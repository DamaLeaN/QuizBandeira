<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tela11">

    <TextView
        android:id="@+id/textView22"
        android:layout_width="411dp"
        android:layout_height="35dp"
        android:background="#050505"
        android:text="Que pais é este?"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <RadioGroup
        android:id="@+id/rdGroup10"
        android:layout_width="231dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView22"
        app:layout_constraintVertical_bias="0.715">

        <RadioButton
            android:id="@+id/rdbHolanda"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="validacao"
            android:text="Holanda" />

        <RadioButton
            android:id="@+id/rdbSuica"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:onClick="validacao"
            android:text="Suiça" />

        <RadioButton
            android:id="@+id/rdbCamaroes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="validacao"
            android:text="Camaraões" />

        <RadioButton
            android:id="@+id/rdbAlemanha"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:onClick="validacao"
            android:text="Alemanha" />
    </RadioGroup>

    <Button
        android:id="@+id/btnResponder10"
        android:layout_width="357dp"
        android:layout_height="61dp"
        android:enabled="false"
        android:onClick="tela11"
        android:text="Responder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.904" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:text="10/10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="358dp"
        android:layout_height="257dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.168"
        app:srcCompat="@drawable/holanda_" />

</androidx.constraintlayout.widget.ConstraintLayout>